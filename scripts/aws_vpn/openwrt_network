config interface 'vti1'
  option proto 'vti'
  option mtu '1500'
  option tunlink 'serrada'
  option peeraddr '46.137.105.8'
  option zone 'vpn'
  option ikey '100'
  option okey '100'

config interface 'vti1_static'
  option proto 'static'
  option device '@vti1'
  list ipaddr '169.254.179.10/30'

config route
  option target '10.0.0.0/8'
  option interface 'vti1_static'
